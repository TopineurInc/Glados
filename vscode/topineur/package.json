{
  "name": "topineur-language",
  "displayName": "Topineur Language Support",
  "description": "Syntax highlighting and language basics for the Topineur paradigm (.top files).",
  "version": "0.1.0",
  "publisher": "topineur",
  "license": "MIT",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "topineur",
        "aliases": [
          "Topineur",
          "top"
        ],
        "extensions": [
          ".top"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "topineur",
        "scopeName": "source.topineur",
        "path": "./syntaxes/topineur.tmLanguage.json"
      }
    ]
  }
}
