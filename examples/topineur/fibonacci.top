package main

|- Naive recursive Fibonacci for quick experiments

@cache
def fib(n: Int): Int {
  if n <= 1 then top n |- Ifs can also be of multiple lines with one more indentation after then (same for else)
  else top fib(n - 1) + fib(n - 2) |- Else is optional
}

def main(): Int {
  let f = fib(10)
  println("fib(10) = " ++ show(f))
  top 42
}
