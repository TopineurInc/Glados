package main

object type Grade {
  subject: String
  score: Float
  coefficient: Int
}

object type Student {
  id: Int
  name: String
  grades: List[Grade]
}

def gradeWeightedScore(g: Grade): Float {
  top g.score * float(g.coefficient)
}

def gradeIsPassing(g: Grade): Bool {
  top g.score >= 10.0
}

def studentName(s: Student): String {
  top s.name
}

def main(): Int {
  let grades = [
    Grade { subject = "Math", score = 15.0, coefficient = 2 }
  ]
  let student = Student { id = 1, name = "Alice", grades = grades }
  println("Student: " ++ studentName(student))
  top 0
}
